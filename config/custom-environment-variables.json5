{
  env: "FENSAK_ENV",
  configFileSizeLimit: {
    __name: "FENSAK_CONFIG_FILE_SIZE_LIMIT",
    __format: "number",
  },
  rulesFileSizeLimit: {
    __name: "FENSAK_RULES_FILE_SIZE_LIMIT",
    __format: "number",
  },
  activeSubscriptionPlanRequired: {
    __name: "FENSAK_ACTIVE_SUBSCRIPTION_PLAN_REQUIRED",
    __format: "boolean",
  },
  planRepoLimits: {
    __name: "FENSAK_PLAN_REPO_LIMITS",
    __format: "json",
  },
  plansAllowedMultipleOrgs: {
    __name: "FENSAK_PLANS_ALLOWED_MULTIPLE_ORGS",
    __format: "json",
  },
  managementAPI: {
    enabled: {
      __name: "FENSAK_MANAGEMENT_API_ENABLED",
      __format: "boolean",
    },
    eventSecretKey: "FENSAK_MANAGEMENT_API_EVENT_SECRET_KEY",
    allowedCORSOrigins: {
      __name: "FENSAK_MANAGEMENT_API_ALLOWED_CORS_ORIGINS",
      __format: "json",
    },
  },
  logging: {
    level: "FENSAK_LOGGING_LEVEL",
    loki: {
      enabled: {
        __name: "FENSAK_LOGGING_LOKI_ENABLED",
        __format: "boolean",
      },
      host: "FENSAK_LOGGING_LOKI_HOST",
      basicAuth: "FENSAK_LOGGING_LOKI_BASIC_AUTH",
    },
  },
  github: {
    allowedOrgs: {
      __name: "FENSAK_GITHUB_ALLOWED_ORGS",
      __format: "json",
    },
    webhookSecret: "FENSAK_GITHUB_WEBHOOK_SECRET",
    app: {
      appID: {
        "__name": "FENSAK_GITHUB_APP_ID",
        "__format": "number",
      },
      privateKey: "FENSAK_GITHUB_APP_PRIVATE_KEY",
    },
    oauthApp: {
      clientID: "FENSAK_GITHUB_OAUTH_APP_CLIENT_ID",
      clientSecret: "FENSAK_GITHUB_OAUTH_APP_CLIENT_SECRET",
    },
    apiToken: "FENSAK_GITHUB_TOKEN",
  },
}
